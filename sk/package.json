{
  "name": "sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "hooks": "vite build -c ./hooks/vite.config.ts",
    "hooks:dev": "vite build --watch -c ./hooks/vite.config.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/pocketbase-types.ts"
  },
  "devDependencies": {
    "@floating-ui/core": "^1.5.0",
    "@iconify-json/ph": "^1.1.7",
    "@iconify-json/simple-icons": "^1.1.80",
    "@julr/unocss-preset-forms": "^0.0.5",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.27.6",
    "@types/node": "^20.10.0",
    "@unocss/extractor-svelte": "^0.57.7",
    "@unocss/reset": "^0.57.7",
    "date-fns": "^2.30.0",
    "pocketbase": "^0.16.0",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.2",
    "svelte-floating-ui": "^1.5.6",
    "svelte-legos": "^0.2.2",
    "tslib": "^2.4.1",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "unocss-preset-theme": "^0.11.0",
    "vite": "^4.5.0"
  },
  "type": "module"
}
